                (distance(x1,y1,x3,y3)<=commrange & distance(x1,y1,x4,y4)<=commrange & distance(x3,y3,x2,y2)<=commrange & distance(x2,y2,x4,y4)<=commrange & N1C3=1 & e1N3C2=1 & e1N2C3=1 & N1C4=1 & e1N4C2=1 & e1N2C4=1) |
                (distance(x1,y1,x3,y3)<=commrange & distance(x3,y3,x2,y2)<=commrange & N1C3=1 & e1N3C2=1 & e1N2C3=1) |
                (distance(x1,y1,x4,y4)<=commrange & distance(x3,y3,x4,y4)<=commrange & N1C4=1 & e1N4C2=1 & e1N2C4=1)
            )
        )   
    )
End.



